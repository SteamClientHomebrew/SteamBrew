{
	"hosting": {
		"target": "steambrew-docs",
		"public": ".vitepress/dist",
		"predeploy": "pnpm run build",
		"ignore": [],
		"cleanUrls": true,
		"redirects": [
			{
				"source": "/users/installing",
				"destination": "/users/getting-started/installation",
				"type": 301
			},
			{
				"source": "/users/uninstalling",
				"destination": "/users/parting-ways/uninstall",
				"type": 301
			}
		],
		"headers": [
			{
				"source": "**",
				"headers": [
					{
						"key": "Cache-Control",
						"value": "public, max-age=1800, s-maxage=1800"
					}
				]
			}
		]
	}
}
