{
	"hosting": {
		"errorPages": {
			"500": "/public/500.html"
		},
		"predeploy": "pnpm run build",
		"target": "steam-brew",
		"source": ".",
		"ignore": [],
		"frameworksBackend": {
			"region": "us-central1"
		},
		"headers": [
			{
				"source": "**",
				"headers": [
					{
						"key": "Cache-Control",
						"value": "public, max-age=1800, s-maxage=1800"
					}
				]
			}
		]
	}
}
